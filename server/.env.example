# MSK Assistant Server Environment Variables

# Server Configuration
PORT=3001
NODE_ENV=development

# Databricks Configuration
DATABRICKS_PAT=your_databricks_pat_here
DATABRICKS_ENDPOINT=https://msk-mode-prod.cloud.databricks.com/serving-endpoints/databricks-claude-sonnet-4/invocations

# CORS Configuration
FRONTEND_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session Storage (for demo - use Redis/Database in production)
SESSION_STORAGE=memory
